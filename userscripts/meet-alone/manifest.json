{
	"name": "Meet Alone",
	"version": "0.1",
	"description": "Force new tabs for meet.google.com into new windows instead.",
    "manifest_version": 2,
    "minimum_chrome_version": "74",
    "applications": {
        "gecko": {
            "id": "meet-alone@snarfed.org",
            "strict_min_version": "67.0"
        }
    },
    "permissions": [
        "*://*.meet.google.com/*",
        "*://localhost/*",
        "tabs"
    ],
    "background": {
        "scripts": ["new-window.js"]
    }
}
